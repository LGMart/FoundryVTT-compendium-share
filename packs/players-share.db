ue":null,"units":""},"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":null,"long":null,"units":"touch"},"uses":{"value":1,"max":1,"per":"charges","autoDestroy":false,"autoUse":true},"consume":{"type":"","target":null,"amount":null},"ability":"","actionType":"util","attackBonus":0,"chatFlavor":"","critical":null,"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"spell"},"consumableType":"trinket","attributes":{"spelldc":10}},"sort":5600000,"flags":{"core":{"sourceId":"Compendium.world.equipamento.ltjk4XFxSUqQJlOO"}},"img":"systems/dnd5e/icons/items/inventory/stake.jpg","effects":[]},{"_id":"CouI9ImRSl2IGxYE","name":"Feature: Safe Heaven","type":"feat","data":{"description":{"value":"<div  class=\"rd__b  rd__b--3\"><p>You have created a second identity that includes documentation, established acquaintances, and disguises that allow you to assume that persona. Additionally, you can forge documents including official papers and personal letters, as long as you have seen an example of the kind of document or the handwriting you are trying to copy.</p><div class=\"rd__spc-inline-post\"></div></div>","chat":"","unidentified":""},"source":"PHB","activation":{"type":"","cost":0,"condition":""},"duration":{"value":null,"units":""},"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":null,"long":null,"units":""},"uses":{"value":0,"max":0,"per":null},"consume":{"type":null,"target":null,"amount":null},"ability":"","actionType":"other","attackBonus":0,"chatFlavor":"","critical":null,"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"flat"},"requirements":"","recharge":{"value":null,"charged":false},"attributes":{"spelldc":10}},"sort":800000,"flags":{},"img":"https://static.wikia.nocookie.net/forgottenrealms/images/3/3b/Harpers_5e.png","effects":[]},{"_id":"1AoxXh2tBwgGsMYJ","name":"Studded Leather Armor","type":"equipment","data":{"description":{"value":"<p>Made from tough but flexible leather, studded leather is reinforced with close-set rivets or spikes.</p>","chat":"","unidentified":""},"source":"PHB pg. 144","quantity":1,"weight":13,"price":45,"attuned":false,"equipped":true,"rarity":"Common","identified":true,"activation":{"type":"","cost":0,"condition":""},"duration":{"value":null,"units":""},"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":null,"long":null,"units":""},"uses":{"value":0,"max":0,"per":null},"consume":{"type":"","target":null,"amount":null},"ability":null,"actionType":"","attackBonus":0,"chatFlavor":"","critical":null,"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"spell"},"armor":{"value":12,"type":"light","dex":null},"hp":{"value":0,"max":0,"dt":null,"conditions":""},"speed":{"value":null,"conditions":""},"strength":null,"stealth":false,"proficient":false,"attributes":{"spelldc":10}},"sort":5700000,"flags":{"core":{"sourceId":"Compendium.world.arsenal.8dNBnxrpie3Z4r8L"},"dae":{"activeEquipped":true}},"img":"systems/dnd5e/icons/items/armor/studded.png","effects":[{"_id":"uQRyibcbwcYwO7qo","flags":{"dae":{"transfer":true,"armorEffect":true}},"changes":[{"key":"data.attributes.ac.value","value":"(@abilities.dex.mod > 99 ? 99 : @abilities.dex.mod)+ 12","mode":5,"priority":4}],"disabled":true,"duration":{},"icon":"systems/dnd5e/icons/items/armor/studded.png","label":"Studded Leather Armor","origin":"Actor.QmGWlKfb4JxzjPye.OwnedItem.8dNBnxrpie3Z4r8L","transfer":true}]},{"_id":"cLLq5S0n02IzOZj0","name":"Goggles of Night","type":"equipment","data":{"description":{"value":"<div>\n\t\t\t<div><i>Wondrous item, minor tier, uncommon</i></div>\n\t\t\t<div class=\"row\">\n\t\t\t\t<div class=\"col-4\"></div>\n\t\t\t\t<div class=\"col-8 text-right\">  </div>\n\t\t\t</div>\n\t\t</div>\n\t\t<hr>\n\t\t<div  class=\"rd__b  rd__b--2\"><p>While wearing these dark lenses, you have <span class=\"help--hover\" onmouseover=\"Renderer.hover.handlePredefinedMouseOver(event, this, 1, {})\" onmousemove=\"Renderer.hover.handlePredefinedMouseMove(event, this)\" onmouseleave=\"Renderer.hover.handlePredefinedMouseLeave(event, this)\" ontouchstart=\"Renderer.hover.handleTouchStart(event, this)\">darkvision</span> out to a range of 60 feet. If you already have <span class=\"help--hover\" onmouseover=\"Renderer.hover.handlePredefinedMouseOver(event, this, 2, {})\" onmousemove=\"Renderer.hover.handlePredefinedMouseMove(event, this)\" onmouseleave=\"Renderer.hover.handlePredefinedMouseLeave(event, this)\" ontouchstart=\"Renderer.hover.handleTouchStart(event, this)\">darkvision</span>, wearing the goggles increases its range by 60 feet.</p></div>","chat":"","unidentified":""},"source":"DMG","quantity":1,"weight":0,"price":null,"attuned":false,"equipped":true,"rarity":"Uncommon","identified":true,"activation":{"type":"","cost":0,"condition":""},"duration":{"value":0,"units":""},"target":{"value":0,"width":null,"units":"","type":""},"range":{"value":null,"long":null,"units":""},"uses":{"value":0,"max":0,"per":""},"consume":{"type":"","target":"","amount":null},"ability":"","actionType":"","attackBonus":0,"chatFlavor":"","critical":null,"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"spell"},"armor":{"value":0,"type":"trinket","dex":0},"hp":{"value":0,"max":0,"dt":null,"conditions":""},"speed":{"value":null,"conditions":""},"strength":0,"stealth":false,"proficient":true,"attributes":{"spelldc":10}},"sort":5800000,"flags":{},"img":"https://5e.tools/img/items%2FGoggles%20of%20Night.jpg","effects":[]},{"_id":"lTyQHUHwSnLWhUi0","name":"Illuminator's Tattoo","type":"equipment","data":{"description":{"value":"<div>\n\t\t\t<div><i>Wondrous item (tattoo), common (requires attunement)</i></div>\n\t\t\t<div class=\"row\">\n\t\t\t\t<div class=\"col-4\"></div>\n\t\t\t\t<div class=\"col-8 text-right\">  </div>\n\t\t\t</div>\n\t\t</div>\n\t\t<hr>\n\t\t<div  class=\"rd__b  rd__b--2\"><p>Produced by a special needle, this magic tattoo features beautiful calligraphy, images of writing implements, and the like.</p><div  class=\"rd__b  rd__b--3\"><span class=\"rd__h rd__h--3\" data-title-index=\"3\" > <span class=\"entry-title-inner\">Tattoo Attunement.</span></span> <p>To attune to this item, you hold the needle to your skin where you want the tattoo to appear, pressing the needle there throughout the attunement process. When the attunement is complete, the needle turns into the ink that becomes the tattoo, which appears on the skin.</p><div class=\"rd__spc-inline-post\"></div><p>If your attunement to the tattoo ends, the tattoo vanishes, and the needle reappears in your space.</p></div><div  class=\"rd__b  rd__b--3\"><span class=\"rd__h rd__h--3\" data-title-index=\"4\" > <span class=\"entry-title-inner\">Magical Scribing.</span></span> <p>While this tattoo is on your skin, you can write with your fingertip as if it were an ink pen that never runs out of ink.</p><div class=\"rd__spc-inline-post\"></div><p>As an action, you can touch a piece of writing up to one page in length and speak a creature's name. The writing becomes <span class=\"bold\" onmouseover=\"Renderer.hover.pHandleLinkMouseOver(event, this, 'conditionsdiseases.html', 'PHB', 'invisible_phb', null)\" onmouseleave=\"Renderer.hover.handleLinkMouseLeave(event, this)\" onmousemove=\"Renderer.hover.handleLinkMouseMove(event, this)\"  ontouchstart=\"Renderer.hover.handleTouchStart(event, this)\" >invisible</span> to everyone other than you and the named creature for the next 24 hours. Either of you can dismiss the invisibility by touching the script (no action required). Once used, this action can't be used again until the next dawn.</p></div></div>","chat":"","unidentified":""},"source":"TCE","quantity":1,"weight":0,"price":null,"attuned":true,"equipped":true,"rarity":"Common","identified":true,"activation":{"type":"","cost":0,"condition":""},"duration":{"value":null,"units":""},"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":null,"long":null,"units":""},"uses":{"value":0,"max":0,"per":null},"consume":{"type":"","target":null,"amount":null},"ability":"","actionType":"","attackBonus":0,"chatFlavor":"","critical":null,"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"spell"},"armor":{"value":null,"type":"","dex":null},"hp":{"value":0,"max":0,"dt":null,"conditions":""},"speed":{"value":null,"conditions":""},"strength":null,"stealth":false,"proficient":true},"sort":5900000,"flags":{"plutonium":{"page":"items.html","source":"TCE","hash":"illuminator's%20tattoo_tce"}},"img":"modules/plutonium/media/icon/crossed-swords.svg","effects":[]},{"_id":"ajWnfjPn7EF042IX","name":"Scout","type":"feat","data":{"description":{"value":"<div  class=\"rd__b  rd__b--1\"><p>You are skilled in stealth and surviving far from the streets of a city, allowing you to scout ahead of your companions during expeditions. Rogues who embrace this archetype are at home in the wilderness and among barbarians and rangers, and many Scouts serve as the eyes and ears of war bands. Ambusher, spy, bounty hunter—these are just a few of the roles that Scouts assume as they range the world.</p></div>","chat":"","unidentified":""},"source":"XGE","activation":{"type":"","cost":0,"condition":""},"duration":{"value":null,"units":""},"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":null,"long":null,"units":""},"uses":{"value":0,"max":0,"per":null},"consume":{"type":null,"target":null,"amount":null},"ability":"","actionType":"other","attackBonus":0,"chatFlavor":"","critical":null,"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"flat"},"requirements":"Rogue 3 (Scout)","recharge":{"value":null,"charged":false},"flags":{"srd5e":{"dataType":"subclassFeature","className":"Rogue","subclassName":"Scout"}}},"sort":6000000,"flags":{},"img":"systems/dnd5e/icons/spells/leaf-jade-3.jpg","effects":[]},{"_id":"s4GBD6CNbccoDICz","name":"Skirmisher","type":"feat","data":{"description":{"value":"<div  class=\"rd__b  rd__b--1\"><p>Starting at 3rd level, you are difficult to pin down during a fight. You can move up to half your speed as a reaction when an enemy ends its turn within 5 feet of you. This movement doesn't provoke opportunity attacks.</p></div>","chat":"","unidentified":""},"source":"XGE","activation":{"type":"","cost":0,"condition":""},"duration":{"value":null,"units":""},"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":null,"long":null,"units":""},"uses":{"value":0,"max":0,"per":null},"consume":{"type":null,"target":null,"amount":null},"ability":"","actionType":"other","attackBonus":0,"chatFlavor":"","critical":null,"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"flat"},"requirements":"Rogue 3 (Scout)","recharge":{"value":null,"charged":false},"flags":{"srd5e":{"dataType":"subclassFeature","className":"Rogue","subclassName":"Scout"}}},"sort":6100000,"flags":{"favtab":{"isFavorite":true}},"img":"systems/dnd5e/icons/spells/leaf-jade-3.jpg","effects":[]},{"_id":"K0AnwKI9kcjiInQm","name":"Survivalist","type":"feat","data":{"description":{"value":"<div  class=\"rd__b  rd__b--1\"><p>When you choose this archetype at 3rd level, you gain proficiency in the <span class=\"help--hover\" onmouseover=\"Renderer.hover.handlePredefinedMouseOver(event, this, 1, {})\" onmousemove=\"Renderer.hover.handlePredefinedMouseMove(event, this)\" onmouseleave=\"Renderer.hover.handlePredefinedMouseLeave(event, this)\" ontouchstart=\"Renderer.hover.handleTouchStart(event, this)\">Nature</span> and <span class=\"help--hover\" onmouseover=\"Renderer.hover.handlePredefinedMouseOver(event, this, 2, {})\" onmousemove=\"Renderer.hover.handlePredefinedMouseMove(event, this)\" onmouseleave=\"Renderer.hover.handlePredefinedMouseLeave(event, this)\" ontouchstart=\"Renderer.hover.handleTouchStart(event, this)\">Survival</span> skills if you don't already have it. Your proficiency bonus is doubled for any ability check you make that uses either of those proficiencies.</p></div>","chat":"","unidentified":""},"source":"XGE","activation":{"type":"","cost":0,"condition":""},"duration":{"value":null,"units":""},"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":null,"long":null,"units":""},"uses":{"value":0,"max":0,"per":null},"consume":{"type":null,"target":null,"amount":null},"ability":"","actionType":"other","attackBonus":0,"chatFlavor":"","critical":null,"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"flat"},"requirements":"Rogue 3 (Scout)","recharge":{"value":null,"charged":false},"flags":{"srd5e":{"dataType":"subclassFeature","className":"Rogue","subclassName":"Scout"}}},"sort":6200000,"flags":{"favtab":{"isFavorite":false}},"img":"systems/dnd5e/icons/spells/leaf-jade-3.jpg","effects":[]},{"_id":"aEEi6djdAlXRE1NS","name":"Steady Aim","type":"feat","data":{"description":{"value":"<div  class=\"rd__b  rd__b--1\"><p><i>3rd-level rogue <span class=\"bold\" onmouseover=\"Renderer.hover.pHandleLinkMouseOver(event, this, 'variantrules.html', 'tce', 'optional%20class%20features_tce', null)\" onmouseleave=\"Renderer.hover.handleLinkMouseLeave(event, this)\" onmousemove=\"Renderer.hover.handleLinkMouseMove(event, this)\"  ontouchstart=\"Renderer.hover.handleTouchStart(event, this)\" >optional feature</span></i></p><p>As a bonus action, you give yourself advantage on your next attack roll on the current turn. You can use this bonus action only if you haven't moved during this turn, and after you use the bonus action, your speed is 0 until the end of the current turn.</p></div>","chat":"","unidentified":""},"source":"TCE","activation":{"type":"bonus","cost":1,"condition":""},"duration":{"value":null,"units":""},"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":null,"long":null,"units":""},"uses":{"value":0,"max":0,"per":null},"consume":{"type":null,"target":null,"amount":null},"ability":"","actionType":"other","attackBonus":0,"chatFlavor":"","critical":null,"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"flat"},"requirements":"Rogue 3","recharge":{"value":null,"charged":false},"flags":{"srd5e":{"dataType":"classFeature","className":"Rogue"}}},"sort":6300000,"flags":{},"img":"modules/plutonium/media/icon/mighty-force.svg","effects":[]}],"effects":[{"_id":"1z9QyTADLHPGWVja","flags":{"dae":{"transfer":true,"armorEffect":true}},"changes":[{"key":"data.attributes.ac.value","value":"(@abilities.dex.mod > 99 ? 99 : @abilities.dex.mod)+ 12","mode":5,"priority":4}],"disabled":false,"duration":{"startTime":0},"icon":"systems/dnd5e/icons/items/armor/studded.png","label":"Studded Leather Armor","origin":"Actor.QmGWlKfb4JxzjPye.OwnedItem.1AoxXh2tBwgGsMYJ"}]}
{"_id":"dgLoEasa8QOYT4Su","name":"Korathok Hornweaver","permission":{"default":0,"0zbqaPRKVgwkfgiZ":3},"type":"character","data":{"abilities":{"str":{"value":16,"proficient":1},"dex":{"value":10,"proficient":0},"con":{"value":16,"proficient":1},"int":{"value":8,"proficient":0},"wis":{"value":13,"proficient":0},"cha":{"value":12,"proficient":0}},"attributes":{"ac":{"value":10},"hp":{"value":13,"min":0,"max":13,"temp":null,"tempmax":null},"init":{"value":0,"bonus":0},"movement":{"burrow":null,"climb":null,"fly":null,"swim":null,"walk":30,"units":"ft","hover":false},"spellcasting":"int","speed":{"_deprecated":true,"value":"30 ft.","special":""},"death":{"success":0,"failure":0},"encumbrance":{"value":null,"max":null},"exhaustion":0,"inspiration":true,"prof":2},"details":{"biography":{"value":"","public":""},"alignment":"Lawful Neutral","race":"Goliath","background":"Gladiator","xp":{"value":0,"min":0,"max":300,"pct":0},"appearance":"","trait":"<p>I&rsquo;ll settle for nothing less than perfection.</p>\n<p>&nbsp;</p>\n<p>Nobody stays angry at me or around me for long, since I can defuse any amount of tension.</p>","ideal":"<p>Tradition. The stories, legends, and songs of the past must never be forgotten, for they teach us who we are. (Lawful)</p>","bond":"<p>Someone stole my precious instrument, and someday I&rsquo;ll get it back.</p>","flaw":"<p>I&rsquo;ll do anything to win fame and renown.</p>","notes1name":"","notes2name":"","notes3name":"","notes4name":""},"traits":{"size":"med","di":{"value":[],"custom":""},"dr":{"value":["cold"],"custom":""},"dv":{"value":[],"custom":""},"ci":{"value":[],"custom":""},"senses":"","languages":{"value":["common","giant"],"custom":""},"weaponProf":{"value":["sim","mar"],"custom":""},"armorProf":{"value":["lgt","med","hvy","shl"],"custom":""},"toolProf":{"value":["disg"],"custom":"Horn"}},"currency":{"pp":0,"gp":0,"ep":0,"sp":0,"cp":0},"skills":{"acr":{"value":1,"ability":"dex"},"ani":{"value":0,"ability":"wis"},"arc":{"value":0,"abil